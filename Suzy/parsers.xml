<?xml version="1.0"?>
<!-- 
	default = normal -api cmd anything else will be 
	-api.<name>
	if no filename is given it will default to <name>.dat
-->
<parsers>
	<parser name="default" fileName="api.dat">
		<api>
			<src>classes/src-zip/</src>
			<packages>java:javax</packages>
			<base>http://java.sun.com/javase/6/docs/api/</base>
		</api>
	</parser>
	
	<parser name="suzy" fileName="api-suzy.dat">
		<api>
			<src>src/</src>
			<packages>de.berlios.suzy</packages>
			<base>http://www.xehonk.de/projects/files/suzy/javadoc/</base>
		</api>
	</parser>
	
	<parser name="lwjgl" fileName="api-lwjgl.dat">
		<api>
			<src>classes/lwjgl-src/</src>
			<packages>org.lwjgl</packages>
			<base>http://lwjgl.org/javadoc/</base>
		</api>
	</parser>

	<!--
	<parser name="ee" fileName="">
		<api>
			<src>jee/classes/src</src>
			<packages>javax</packages>
			<base>http://java.sun.com/j2ee/1.4/docs/api/</base>
		</api>
	</parser>
	-->
	
</parsers>
